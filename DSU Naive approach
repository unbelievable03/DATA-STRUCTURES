int parent[N];
void make_set(int v)
{
    parent[v]=v;
}
int find_set(int v).         // time complexity of find set is O(N) as consider the case of long chain as it will visit all vertices
{
    if(v==parent[v])
    {
        return v;
    }
    return find_set(parent[v]);
}
void union_sets(int a, int b)
{
    a= find_set(a);
    b=find_set(b);
    if(a!=b)
    {
        parent[b]=a;
    }
}
runs in O(N) time complexity
